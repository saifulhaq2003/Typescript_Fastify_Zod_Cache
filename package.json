  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only src/entry/server.ts",
    "start": "node dist/entry/server.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -d src/app/database/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create src/app/database/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm -- migration:generate src/app/database/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
